  "license": "GPL-3.0-only",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/vscode": "^1.105.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
